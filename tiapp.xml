<?xml version="1.0" encoding="UTF-8"?>
<ti:app xmlns:ti="http://ti.appcelerator.org">
    <id>org.cchd.podapp</id>
    <name>POD Assistant</name>
    <version>1.0.2</version>
    <publisher>Southern Nevada Health District</publisher>
    <url>https://southernnevadahealthdistrict.org/</url>
    <description>This app is to be used by personnel at POD (Point of Dispensing) sites to dispense medicine. A POD is a location where you can go to get medications or vaccines during a public health emergency.</description>
    <copyright>2015 by Southern Nevada Health District</copyright>
    <icon>appicon.png</icon>
    <fullscreen>false</fullscreen>
    <navbar-hidden>false</navbar-hidden>
    <analytics>true</analytics>
    <guid>5a7f72af-86e2-469d-9000-e7e4761a8652</guid>
    <property name="ti.ui.defaultunit" type="string">dp</property>
    <android xmlns:android="http://schemas.android.com/apk/res/android">
        <manifest android:versionCode="102" android:versionName="1.0.2">
            <uses-sdk android:minSdkVersion="17" android:targetSdkVersion="21"/>
        </manifest>
    </android>
    <mobileweb>
        <precache/>
        <splash>
            <enabled>true</enabled>
            <inline-css-images>true</inline-css-images>
        </splash>
        <theme>default</theme>
    </mobileweb>
    <modules>
        <module platform="android">analytics.google</module>
        <module platform="android" version="2.2.1">ti.imagefactory</module>
        <module platform="android">com.acktie.mobile.android.qr</module>
    </modules>
    <deployment-targets>
        <target device="android">true</target>
        <target device="ipad">false</target>
        <target device="iphone">false</target>
        <target device="mobileweb">false</target>
        <target device="windows">false</target>
    </deployment-targets>
    <sdk-version>5.0.0.GA</sdk-version>
    <plugins>
        <plugin version="1.0">ti.alloy</plugin>
    </plugins>
    <property name="appc-app-id" type="string">55d2381365df2c956dcd5072</property>
</ti:app>
